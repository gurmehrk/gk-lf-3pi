target C {
    platform: {
      name: "rp2040",
      board: "pololu_3pi_2040_robot"
    },
    single-threaded: true,
  }

import LED from "LED.lf"

main reactor ToolsLEDSolution {
  timer t_on(0, 500 ms)
  timer t_off(250 ms, 500 ms)
  l = new LED()

  reaction(t_on) -> l.set{=
    lf_set(l.set, true);
  =}

  reaction(t_off) -> l.set{=
    lf_set(l.set, false);
  =}
}